<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.nossotatame" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Nosso Tatame</name>
    <description>
        Assista técnicas, veja lutas e marque sua pontuação e cronometre suas lutas de Jiu-Jitsu agora! Baixe agora e entre no magnífico mundo do Jiu-Jitsu você também!
    </description>
    <author email="caiodotdev@gmail.com" href="http://www.caiomarinho.com">
        Caio Marinho
    </author>
    <icon src="img/logo.jpg" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
